{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_NAME": "opencode-mobile (Dev)",
        "EXPO_PUBLIC_BUNDLE_ID": "com.goniz.opencodemobile.dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_NAME": "opencode-mobile (Preview)",
        "EXPO_PUBLIC_BUNDLE_ID": "com.goniz.opencodemobile.preview"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6751230804"
      }
    }
  }
}
